; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BoomerangBounce"
run/main_scene="res://scenes/Game.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://assets/sprites/player.svg"

[autoload]

ViewportOptions="*res://scripts/viewport_options.gd"

[display]

window/size/viewport_width=540
window/size/viewport_height=960
window/handheld/orientation=1
window/stretch/mode="viewport"
window/handheld/emulate_touchscreen=true
window/handheld/touchscreen_button_visibility=false
window/handheld/touchscreen_control_view_show=false
window/handheld/touchscreen_control_view_opacity=0.0

[input_devices]

pointing/emulate_touch_from_mouse=false
pointing/emulate_mouse_from_touch=false

[rendering]

renderer/rendering_method="gl_compatibility"
